
# AgriSaarthi Environment Variables
# Copy this file to .env and fill in your actual API keys

# Speech-to-Text API Configuration
# Options: OpenAI Whisper, AssemblyAI, Google Speech-to-Text
VITE_STT_API_KEY=your_speech_to_text_api_key
VITE_STT_API_URL=https://api.openai.com/v1/audio/transcriptions

# GPT API Configuration  
# Options: OpenAI GPT, Anthropic Claude, Google Gemini
VITE_GPT_API_KEY=your_gpt_api_key
VITE_GPT_API_URL=https://api.openai.com/v1/chat/completions
VITE_GPT_MODEL=gpt-4

# Computer Vision API Configuration
# Options: Roboflow, Google Vision, Custom CV models
VITE_CV_API_KEY=your_computer_vision_api_key
VITE_CV_API_URL=https://detect.roboflow.com/your-model

# Text-to-Speech API Configuration
# Options: ElevenLabs, OpenAI TTS, AWS Polly
VITE_TTS_API_KEY=your_text_to_speech_api_key
VITE_TTS_API_URL=https://api.elevenlabs.io/v1/text-to-speech

# Webhook URLs for External Integrations
# Zapier Webhook URL for farmer chat
VITE_ZAPIER_FARMER_WEBHOOK=https://hooks.zapier.com/hooks/catch/YOUR_FARMER_HOOK/

# Zapier Webhook URL for gardener chat  
VITE_ZAPIER_GARDENER_WEBHOOK=https://hooks.zapier.com/hooks/catch/YOUR_GARDENER_HOOK/

# Make.com Webhook URLs
VITE_MAKE_FARMER_WEBHOOK=https://hook.integromat.com/YOUR_FARMER_SCENARIO
VITE_MAKE_GARDENER_WEBHOOK=https://hook.integromat.com/YOUR_GARDENER_SCENARIO

# Weather API (for farmer context)
VITE_WEATHER_API_KEY=your_weather_api_key
VITE_WEATHER_API_URL=https://api.openweathermap.org/data/2.5

# Market Data API (for farmer context)
VITE_MARKET_API_KEY=your_market_data_api_key
VITE_MARKET_API_URL=https://api.marketdata.example.com

# Optional: Firebase Config for user data persistence
VITE_FIREBASE_API_KEY=your_firebase_api_key
VITE_FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your-project-id

# Optional: Supabase Config for backend
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key
